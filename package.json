{
  "name": "@travelport-czech/flight-watchdog-client-sa",
  "version": "1.6.16",
  "description": "CEE Systems Watchdog Client",
  "main": "dist-emails/emails/src/index.js",
  "types": "dist-emails/emails/src/index.d.ts",
  "files": [
    "dist-emails/**/*",
    "dist-client/**/*"
  ],
  "engines": {
    "node": ">=8.10"
  },
  "scripts": {
    "prepublishOnly": "npm run build-prod",
    "lang-export": "webpack-cli --config app/translationScripts/webpack.config.js && node .dist-translation-scripts/export.js",
    "lang-import": "webpack-cli --config app/translationScripts/webpack.config.js && node .dist-translation-scripts/import.js",
    "lint-client": "tsc --project app/client/tsconfig.json && tslint --project app/client/tsconfig.json --config app/client/tslint.json",
    "lint-emails": "tsc --project app/emails/tsconfig.json && tslint --project app/emails/tsconfig.json --config app/emails/tslint.json",
    "lint": "npm run lint-client && npm run lint-emails",
    "autofix": "prettier app/**/* --write --config prettier.config.js",
    "build-emails-test-watch": "webpack-cli --config app/emails/webpack.example.config.js --watch",
    "build-emails-test": "webpack-cli --config app/emails/webpack.example.config.js",
    "build-emails-prod-types": "gulp --gulpfile app/emails/gulpfile.js",
    "build-emails-prod": "tsc --project app/emails/tsconfig.json && npm run build-emails-prod-types",
    "build-client-test": "cross-env NODE_ENV=test webpack-cli --config app/client/webpack.config.js",
    "build-client-test-watch": "cross-env NODE_ENV=test webpack-cli --config app/client/webpack.config.js --watch",
    "build-client-prod": "cross-env NODE_ENV=production webpack-cli --config app/client/webpack.config.js",
    "build-prod": "npm run build-emails-prod && npm run build-client-prod",
    "http-server": "node tests-utils/startHttpServer.js",
    "api-server": "node tests-utils/startApiServer.js",
    "cypress-open": "cypress open",
    "cypress-run": "cypress run",
    "test-open": "concurrently -k -s first \"npm:build-client-test-watch\" \"npm:build-emails-test-watch\" \"npm:http-server\" \"npm:api-server\" \"npm:cypress-open\"",
    "test": "npm run lint-client && npm run build-client-test && npm run build-emails-test && concurrently -k -s first \"npm:http-server\" \"npm:api-server\" \"wait-on http://localhost:8080/ && npm run cypress-run\"",
    "test-ci": "npm run lint-client && npm run build-client-test && npm run build-emails-test && concurrently -k -s first \"npm:http-server\" \"npm:api-server\" \"wait-on http://localhost:8080/ && npm run cypress-run -- --browser chrome\"",
    "open": "concurrently \"npm:build-client-test-watch\" \"npm:build-emails-test-watch\" \"npm:http-server\" \"npm:api-server\" \"opn http://localhost:8080\""
  },
  "dependencies": {
    "@sentry/browser": "^5.3.0",
    "@travelport-czech/valid-objects-ts": "^1.1.4",
    "bufferutil": "^4.0.1",
    "dayjs": "^1.8.16",
    "email-validator": "^2.0.4",
    "extendable-error": "^0.1.5",
    "hash-files": "^1.1.1",
    "js-cookie": "^2.2.0",
    "number-format.js": "^2.0.8",
    "numeral": "^2.0.6",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "recharts": "^1.6.2",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "utf-8-validate": "^5.0.2",
    "uuid": "^3.3.2",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@hapi/hapi": "^18.4.0",
    "@hapi/hoek": "^8.3.1",
    "@hapi/inert": "^5.2.2",
    "@types/fs-extra": "^7.0.0",
    "@types/google.analytics": "^0.0.39",
    "@types/js-cookie": "^2.2.2",
    "@types/node": "^12.0.2",
    "@types/numeral": "^0.0.25",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.4",
    "@types/recharts": "^1.1.16",
    "@types/request-promise-native": "^1.0.16",
    "@types/uuid": "^3.4.4",
    "@types/valid-url": "^1.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-latest-node": "^3.2.1",
    "concurrently": "^4.1.1",
    "cross-env": "^5.2.0",
    "cypress": "^3.4.1",
    "dotenv-webpack": "^1.7.0",
    "fs-extra": "7.0.1",
    "gulp": "^4.0.2",
    "html-loader": "^0.5.5",
    "opn-cli": "^4.1.0",
    "path-alias-resolver": "git+https://github.com/Travelport-Czech/path-alias-resolver.git#v1.0.5",
    "prettier": "^1.18.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-consistent-codestyle": "^1.15.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-immutable": "^5.5.2",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0",
    "ts-loader": "^6.0.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.6.4",
    "wait-on": "^3.3.0",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.6",
    "webpack-visualizer-plugin": "^0.1.11",
    "xlsx": "^0.14.4"
  },
  "author": "C.E.E. group Travelport a.s.",
  "license": "MIT"
}
