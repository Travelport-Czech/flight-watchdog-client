{
  "name": "@ceesystems/flight-watchdog-client",
  "version": "1.2.2",
  "description": "CEE Systems Watchdog Client",
  "main": "README.md",
  "engines": {
    "node": ">=8.10"
  },
  "scripts": {
    "lint": "tsc && tslint --project tsconfig.json --config tslint.json",
    "autofix": "prettier src/**/* --write --single-quote --no-semi --print-width 120 --end-of-line lf",
    "build-test": "cross-env NODE_ENV=test webpack-cli --config src/client/webpack.config.js",
    "build-test-watch": "cross-env NODE_ENV=test webpack-cli --config src/client/webpack.config.js --watch",
    "build-devel": "cross-env NODE_ENV=devel webpack-cli --config src/client/webpack.config.js",
    "build-prod": "cross-env NODE_ENV=prod webpack-cli --config src/client/webpack.config.js",
    "http-server": "node tests-utils/startHttpServer.js",
    "api-server": "node tests-utils/startApiServer.js",
    "cypress-open": "cypress open",
    "cypress-run": "cypress run",
    "cypress-run-ci": "cypress run --record --key 0ce2f32f-fc1e-4051-a454-df2a1e61d145",
    "test-open": "concurrently -k -s first \"npm:build-test-watch\" \"npm:http-server\" \"npm:api-server\" \"npm:cypress-open\"",
    "test": "npm run lint && npm run build-test && concurrently -k -s first \"npm:http-server\" \"npm:api-server\" \"npm:cypress-run\"",
    "test-ci": "npm run lint && npm run build-test && concurrently -k -s first \"npm:http-server\" \"npm:api-server\" \"npm:cypress-run-ci\"",
    "open": "concurrently \"npm:build-test-watch\" \"npm:http-server\" \"npm:api-server\" \"opn http://localhost:8080\""
  },
  "dependencies": {
    "@babel/cli": "7.1.5",
    "@babel/core": "7.1.5",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.1.5",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.1.0",
    "@sentry/browser": "4.6.3",
    "@types/fs-extra": "5.0.4",
    "@types/google.analytics": "0.0.39",
    "@types/js-beautify": "1.8.0",
    "@types/js-cookie": "2.2.0",
    "@types/node": "10.12.0",
    "@types/numeral": "0.0.25",
    "@types/react": "16.7.18",
    "@types/react-dom": "16.0.9",
    "@types/request-promise-native": "1.0.15",
    "@types/uuid": "3.4.4",
    "@types/valid-url": "1.0.2",
    "babel-loader": "8.0.4",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-preset-latest-node": "2.0.2",
    "bufferutil": "4.0.1",
    "compression-webpack-plugin": "2.0.0",
    "cross-env": "5.2.0",
    "cross-fetch": "2.2.3",
    "cross-spawn": "6.0.5",
    "dotenv-webpack": "1.5.7",
    "email-validator": "2.0.4",
    "extendable-error": "0.1.5",
    "fs-extra": "7.0.1",
    "hash-files": "1.1.1",
    "html-loader": "0.5.5",
    "inert": "5.1.2",
    "js-beautify": "1.8.9",
    "js-cookie": "2.2.0",
    "moment": "2.24.0",
    "number-format.js": "2.0.7",
    "numeral": "2.0.6",
    "react": "16.6.1",
    "react-dom": "16.6.1",
    "request": "2.88.0",
    "request-promise-native": "1.0.5",
    "typescript": "3.3.3",
    "utf-8-validate": "5.0.2",
    "uuid": "3.3.2",
    "valid-url": "1.0.9",
    "webpack": "4.22.0",
    "webpack-cli": "3.1.2"
  },
  "devDependencies": {
    "@types/xlsx": "0.0.36",
    "concurrently": "4.1.0",
    "cypress": "3.1.5",
    "hapi": "18.1.0",
    "hapi-cors": "^1.0.3",
    "opn-cli": "^4.0.0",
    "prettier": "1.16.4",
    "tslint": "5.13.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-consistent-codestyle": "1.15.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-immutable": "5.3.0",
    "tslint-microsoft-contrib": "6.0.0",
    "tslint-plugin-prettier": "2.0.1",
    "tslint-react": "3.6.0",
    "xlsx": "0.14.1"
  },
  "author": "C.E.E. group Travelport a.s.",
  "license": "ISC",
  "private": true
}
