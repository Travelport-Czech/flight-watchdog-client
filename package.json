{
  "name": "@travelport-czech/flight-watchdog-client-d4",
  "version": "2.0.16",
  "description": "CEE Systems Watchdog Client",
  "main": "dist-emails/emails/src/index.js",
  "types": "dist-emails/emails/src/index.d.ts",
  "files": [
    "dist-emails/**/*",
    "dist-client/**/*",
    "tests-utils/**/*"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prepublishOnly": "npm run build-prod",
    "lang-export": "esr ./app/translationScripts/exportTranslations.ts",
    "lang-import": "esr ./app/translationScripts/importTranslations.ts",
    "lint-client": "tsc --project app/client/tsconfig.json && eslint '*/**/*.ts' --quiet",
    "lint-emails": "tsc --project app/emails/tsconfig.json && eslint '*/**/*.ts' --quiet",
    "lint": "npm run lint-client && npm run lint-emails",
    "autofix": "prettier app/**/* --write --config prettier.config.js",
    "build-emails-test-watch": "webpack-cli --config app/emails/webpack.example.config.js --watch",
    "build-emails-test": "webpack-cli --config app/emails/webpack.example.config.js",
    "build-emails-prod": "tsc --project app/emails/tsconfig.json && tscpaths -p app/emails/tsconfig.json -s app -o ./dist-emails",
    "build-client-test": "NODE_ENV=test webpack-cli --config app/client/webpack.config.js",
    "build-client-test-watch": "NODE_ENV=test webpack-cli --config app/client/webpack.config.js --watch",
    "build-client-prod": "NODE_ENV=production webpack-cli --config app/client/webpack.config.js",
    "build-prod": "npm run build-emails-prod && npm run build-client-prod",
    "http-server": "node tests-utils/startHttpServer.js",
    "api-server": "node tests-utils/startApiServer.js",
    "cypress-open": "cypress open",
    "cypress-run": "cypress run",
    "test-open": "concurrently -k -s first \"npm:build-client-test-watch\" \"npm:build-emails-test-watch\" \"npm:http-server\" \"npm:api-server\" \"npm:cypress-open\"",
    "test": "npm run lint-client && npm run build-client-test && npm run build-emails-test && concurrently -k -s first \"npm:http-server\" \"npm:api-server\" \"wait-on http://127.0.0.1:8080/ && npm run cypress-run\"",
    "test-ci": "npm run lint-client && npm run build-client-test && npm run build-emails-test && concurrently -k -s first \"npm:http-server\" \"npm:api-server\" \"wait-on http://127.0.0.1:8080/ && npm run cypress-run -- --browser chrome\"",
    "open": "concurrently \"npm:build-client-test-watch\" \"npm:build-emails-test-watch\" \"npm:http-server\" \"npm:api-server\" \"opn http://localhost:8080\""
  },
  "dependencies": {
    "@sentry/browser": "5.30.0",
    "@travelport-czech/ss-translations": "1.1.19",
    "@travelport-czech/valid-objects-ts": "6.1.4",
    "bufferutil": "4.0.8",
    "dayjs": "1.11.10",
    "email-validator": "2.0.4",
    "extendable-error": "0.1.7",
    "hash-files": "1.1.1",
    "js-cookie": "2.2.1",
    "number-format.js": "2.0.9",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "recharts": "2.9.3",
    "request": "2.88.2",
    "request-promise-native": "1.0.9",
    "utf-8-validate": "5.0.10",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@babel/cli": "7.21.0",
    "@babel/core": "7.21.4",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "7.20.7",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.23.2",
    "@babel/preset-react": "7.22.15",
    "@babel/preset-typescript": "7.23.2",
    "@hapi/hapi": "20.3.0",
    "@hapi/hoek": "11.0.2",
    "@hapi/inert": "7.1.0",
    "@travelport-czech/ss-translations-tools": "1.0.25",
    "@types/fs-extra": "9.0.13",
    "@types/google.analytics": "0.0.39",
    "@types/js-cookie": "2.2.7",
    "@types/node": "12.20.55",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.4",
    "@types/recharts": "1.8.26",
    "@types/request-promise-native": "1.0.20",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.58.0",
    "babel-loader": "8.2.2",
    "babel-plugin-module-resolver": "5.0.0",
    "babel-preset-latest-node": "5.5.1",
    "concurrently": "5.3.0",
    "cypress": "13.4.0",
    "dotenv-webpack": "6.0.4",
    "eslint": "8.53.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-cypress": "2.13.2",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "fs-extra": "7.0.1",
    "html-loader": "1.3.2",
    "opn-cli": "5.0.0",
    "prettier": "2.8.7",
    "thread-loader": "3.0.4",
    "ts-loader": "8.4.0",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "tscpaths": "0.0.9",
    "typescript": "4.9.5",
    "wait-on": "7.2.0",
    "webpack": "5.78.0",
    "webpack-cli": "5.0.1",
    "esbuild": "0.17.19",
    "esbuild-runner": "2.2.2"
  },
  "author": "C.E.E. group Travelport a.s.",
  "license": "MIT"
}
